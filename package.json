{
  "name": "erxes-integrations",
  "version": "0.15.2",
  "description": "erxes integrations",
  "homepage": "https://erxes.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/erxes/erxes-integrations"
  },
  "bugs": "https://github.com/erxes/erxes-integrations/issues",
  "keywords": [
    "node",
    "express",
    "graphql",
    "apollo"
  ],
  "license": "GNU General Public License v3.0 with Commons Clause",
  "private": true,
  "scripts": {
    "start": "node dist",
    "dev": "DEBUG=erxes* node_modules/.bin/ts-node-dev --respawn --ignore-watch node_modules src",
    "test": "NODE_ENV=test node --expose-gc --max_old_space_size=4000 ./node_modules/.bin/jest --runInBand --forceExit --silent --logHeapUsage",
    "build": "tsc -p tsconfig.prod.json",
    "lint": "tslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "precommit": "lint-staged",
    "migrate": "NODE_ENV=command migrate --migrations-dir='./dist/migrations' --store='./db-migrate-store.js' up",
    "customCommand": "ts-node ./src/commands/customCommand.ts",
    "resetMigrations": "ts-node ./src/commands/resetMigrations.ts",
    "release": "release-it"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@google-cloud/pubsub": "^1.1.5",
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.0",
    "@types/mongodb": "^3.1.2",
    "@types/mongoose": "^5.2.1",
    "@types/node": "^10.12.18",
    "amqplib": "^0.5.5",
    "body-parser": "^1.17.1",
    "botbuilder": "^4.4.0",
    "botbuilder-adapter-facebook-erxes": "^1.0.5",
    "debug": "^4.1.1",
    "dotenv": "^4.0.0",
    "erxes-inmemory-storage": "^1.0.16",
    "erxes-message-broker": "^1.0.17",
    "express": "^4.16.4",
    "fbgraph": "^1.4.4",
    "formidable": "^1.2.1",
    "gmail-api-parse-message": "^2.1.2",
    "meteor-random": "^0.0.3",
    "migrate": "^1.6.2",
    "mongodb-memory-server": "^6.6.0",
    "mongoose": "5.7.5",
    "nylas": "^4.7.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sanitize-html": "^1.20.1",
    "sinon": "^7.5.0",
    "smooch-core": "^8.8.0",
    "telnyx": "^1.7.2",
    "ts-node": "8.0.3",
    "underscore": "^1.10.2",
    "uuid": "^3.3.3"
  },
  "peerOptionalDependencies": {
    "kerberos": "^1.0.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^1.1.4",
    "husky": "^0.13.4",
    "jest": "22.0.4",
    "lint-staged": "^3.6.0",
    "prettier": "^1.19.1",
    "release-it": "^12.4.3",
    "ts-jest": "22.0.4",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=10.x.x"
  }
}
